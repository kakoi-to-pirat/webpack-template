{
  "name": "pirate-webpack-template",
  "version": "1.0.0",
  "description": "webpack template",
  "author": "Some Pirate",
  "license": "MIT",
  "keywords": [
    "webpack",
    "template"
  ],
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.dev.conf.js",
    "build": "webpack --config ./build/webpack.prod.conf.js",
    "dev": "webpack --config ./build/webpack.dev.conf.js --progress",
    "lint": "eslint --ext .js,.ts .",
    "fix": "eslint --ext .js,.ts . --fix",
    "lint:css": "stylelint \"**/*.(s)css\"",
    "fix:css": "stylelint \"**/*.(s)css\" --fix"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --ext .js,.ts .",
      "git add"
    ],
    "*.{scss,css}": [
      "stylelint \"**/*.(s)css\"",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "csv-loader": "^3.0.3",
    "dotenv-webpack": "^5.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-alpha.3 ",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.1",
    "papaparse": "^5.3.0",
    "path": "^0.12.7",
    "postcss": "^8.1.9",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.0",
    "prettier-eslint": "^12.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "lodash": "^4.17.20"
  }
}
