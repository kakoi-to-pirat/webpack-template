{
  "parser": "babel-eslint",
  "plugins": ["prettier", "jest"],
  "extends": ["airbnb/base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-debugger": "off",
    "no-console": 0
  }
}
